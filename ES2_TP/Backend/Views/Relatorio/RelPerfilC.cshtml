@model BusinessLogic.Entities.Perfil
@{
    ViewBag.Title = "Relatório";
    Layout = "_Layout2";
}

<h2 class="text-center"> Relatório talento</h2>
<div class="row">
    <div class="col-md-4">
        <form asp-action="RelT">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Pais" class="control-label">País</label>
                <input asp-for="Pais" class="form-control"/>
                <span asp-validation-for="Pais" class="text-danger"></span>
            </div>
            <br/>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

